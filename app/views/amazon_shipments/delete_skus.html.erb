<div class="row">
  <div class="col-sm">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3">
      <h1 class="h2">Amazon Shipments Delete Quantities</h1>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm">
    <div class="custom-file">
      <%= form_with(url: {action: :delete_skus}, multipart: true) do %>
      <%= file_field_tag 'csv_file' %>
      <%= submit_tag "Upload", class: 'btn btn-secondary', id: 'import-csv-submit' %>
      <% end %>
    </div>
  </div>
</div>